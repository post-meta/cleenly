### `app/[city]/[service]/page.tsx` — City + Service Page (ГЛАВНАЯ)

```typescript
import { notFound } from 'next/navigation';
import Link from 'next/link';
import { Metadata } from 'next';
import { cities, getCityBySlug } from '@/lib/data/cities';
import { services, getServiceBySlug } from '@/lib/data/services';
import { JsonLd } from '@/components/shared/json-ld';
import { 
  generateServiceSchema, 
  generateFAQSchema, 
  generateBreadcrumbSchema 
} from '@/lib/utils/schema-generators';
import { 
  generateIntroText, 
  generateWhyChooseText, 
  generateLocalFAQs 
} from '@/lib/utils/content-generator';

interface PageProps {
  params: { city: string; service: string };
}

// Генерируем все 66 комбинаций при билде
export async function generateStaticParams() {
  const params = [];
  for (const city of cities) {
    for (const service of services) {
      params.push({ city: city.slug, service: service.slug });
    }
  }
  return params;
}

export async function generateMetadata({ params }: PageProps): Promise<Metadata> {
  const city = getCityBySlug(params.city);
  const service = getServiceBySlug(params.service);
  if (!city || !service) return {};

  return {
    title: `${service.name} ${city.name} WA | ${service.priceRange} | CLEENLY`,
    description: `${service.name} in ${city.name}, Washington. ${service.priceRange}. Serving ${city.neighborhoods.slice(0, 3).join(', ')}. Book online in 2 minutes.`,
    alternates: {
      canonical: `https://cleenly.com/${city.slug}/${service.slug}`,
    },
  };
}

export default function CityServicePage({ params }: PageProps) {
  const city = getCityBySlug(params.city);
  const service = getServiceBySlug(params.service);
  
  if (!city || !service) notFound();

  const introText = generateIntroText(city, service);
  const whyChooseText = generateWhyChooseText(city, service);
  const localFaqs = generateLocalFAQs(city, service);

  const serviceSchema = generateServiceSchema(city, service);
  const faqSchema = generateFAQSchema(localFaqs);
  const breadcrumbSchema = generateBreadcrumbSchema([
    { name: 'Home', url: 'https://cleenly.com' },
    { name: city.name, url: `https://cleenly.com/${city.slug}` },
    { name: service.name }
  ]);

  return (
    <main>
      <JsonLd data={serviceSchema} />
      <JsonLd data={faqSchema} />
      <JsonLd data={breadcrumbSchema} />

      {/* Breadcrumbs */}
      <nav className="border-b border-border">
        <div className="max-w-6xl mx-auto px-6 py-3 text-sm text-muted-foreground">
          <Link href="/" className="hover:text-foreground">Home</Link>
          <span className="mx-2">/</span>
          <Link href={`/${city.slug}`} className="hover:text-foreground">{city.name}</Link>
          <span className="mx-2">/</span>
          <span className="text-foreground">{service.name}</span>
        </div>
      </nav>

      {/* Hero — Первые 100 слов оптимизированы для AI */}
      <section className="py-16 md:py-24">
        <div className="max-w-6xl mx-auto px-6">
          <h1 className="text-4xl md:text-5xl font-semibold tracking-tight">
            {service.name} in {city.name}, WA
          </h1>
          <p className="mt-6 text-xl text-muted-foreground max-w-3xl">
            {introText}
          </p>
          <div className="mt-8 flex flex-col sm:flex-row gap-4">
            <Link 
              href="/book"
              className="inline-block bg-foreground text-background px-8 py-4 rounded-lg font-medium hover:bg-foreground/90 transition-colors text-center"
            >
              Book Now — {service.priceRange}
            </Link>
            <Link 
              href={`/${city.slug}`}
              className="inline-block border border-border px-8 py-4 rounded-lg font-medium hover:bg-muted transition-colors text-center"
            >
              All Services in {city.name}
            </Link>
          </div>
        </div>
      </section>

      {/* Pricing Table — AI оптимизировано */}
      <section className="py-12 bg-muted">
        <div className="max-w-3xl mx-auto px-6">
          <h2 className="text-2xl font-semibold mb-6">{service.name} Prices in {city.name}</h2>
          <div className="bg-background rounded-lg border border-border overflow-hidden">
            <table className="w-full">
              <thead className="bg-muted/50">
                <tr>
                  <th className="text-left p-4 font-medium">Service</th>
                  <th className="text-left p-4 font-medium">Price</th>
                  <th className="text-left p-4 font-medium">Duration</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td className="p-4 font-medium">{service.name}</td>
                  <td className="p-4 font-semibold">{service.priceRange}</td>
                  <td className="p-4 text-muted-foreground">{service.duration}</td>
                </tr>
              </tbody>
            </table>
          </div>
          <p className="mt-4 text-sm text-muted-foreground">
            Prices for {city.name} homes. Final price depends on home size and condition.
          </p>
        </div>
      </section>

      {/* What's Included */}
      <section className="py-16">
        <div className="max-w-6xl mx-auto px-6">
          <div className="grid lg:grid-cols-2 gap-12">
            <div>
              <h2 className="text-2xl font-semibold mb-6">What's Included</h2>
              <div className="space-y-3">
                {service.checklist.map((item, i) => (
                  <div key={i} className="flex items-start gap-3">
                    <span className="text-foreground font-medium mt-0.5">✓</span>
                    <span>{item}</span>
                  </div>
                ))}
              </div>
              
              <div className="mt-8 p-4 border border-border rounded-lg">
                <h3 className="font-medium mb-2">Not included:</h3>
                <ul className="text-sm text-muted-foreground space-y-1">
                  {service.notIncluded.map((item, i) => (
                    <li key={i}>• {item}</li>
                  ))}
                </ul>
              </div>
            </div>
            
            <div>
              <h2 className="text-2xl font-semibold mb-6">Why {city.name} Homeowners Choose CLEENLY</h2>
              <p className="text-muted-foreground mb-6">
                {service.longDescription}
              </p>
              <p className="text-muted-foreground mb-6">
                {whyChooseText}
              </p>
              
              <div className="p-6 bg-muted rounded-lg">
                <h3 className="font-semibold mb-3">Best for:</h3>
                <ul className="space-y-2">
                  {service.bestFor.map((item, i) => (
                    <li key={i} className="flex items-center gap-2">
                      <span>•</span>
                      <span>{item}</span>
                    </li>
                  ))}
                </ul>
              </div>
            </div>
          </div>
        </div>
      </section>

      {/* FAQ — Voice Search Optimized */}
      <section className="py-16 bg-muted">
        <div className="max-w-3xl mx-auto px-6">
          <h2 className="text-2xl font-semibold mb-8">
            {service.name} FAQ for {city.name}
          </h2>
          <div className="space-y-4">
            {localFaqs.map((faq, i) => (
              <div key={i} className="bg-background p-6 rounded-lg border border-border">
                <h3 className="font-semibold mb-2">{faq.question}</h3>
                <p className="text-muted-foreground">{faq.answer}</p>
              </div>
            ))}
          </div>
        </div>
      </section>

      {/* Neighborhoods Served */}
      <section className="py-16">
        <div className="max-w-6xl mx-auto px-6">
          <h2 className="text-xl font-semibold mb-4">
            {service.name} available in all {city.name} neighborhoods
          </h2>
          <div className="flex flex-wrap gap-2">
            {city.neighborhoods.map(hood => (
              <span 
                key={hood}
                className="px-3 py-1 bg-muted rounded-full text-sm"
              >
                {hood}
              </span>
            ))}
          </div>
        </div>
      </section>

      {/* Nearby Areas — Internal Linking */}
      <section className="py-12 border-t border-border">
        <div className="max-w-6xl mx-auto px-6">
          <p className="text-sm text-muted-foreground mb-4">
            Also serving nearby:
          </p>
          <div className="flex flex-wrap gap-4">
            {city.nearbyAreas.map(areaSlug => {
              const nearbyCity = getCityBySlug(areaSlug);
              if (!nearbyCity) return null;
              return (
                <Link
                  key={areaSlug}
                  href={`/${areaSlug}/${service.slug}`}
                  className="text-sm underline underline-offset-4 hover:no-underline"
                >
                  {service.name} in {nearbyCity.name}
                </Link>
              );
            })}
          </div>
        </div>
      </section>

      {/* Final CTA */}
      <section className="py-16 bg-foreground text-background">
        <div className="max-w-6xl mx-auto px-6 text-center">
          <h2 className="text-2xl font-semibold mb-4">
            Ready to book {service.name.toLowerCase()} in {city.name}?
          </h2>
          <p className="text-background/70 mb-8">
            See your exact price. Pick a time. Book in 2 minutes.
          </p>
          <Link 
            href="/book"
            className="inline-block bg-background text-foreground px-8 py-4 rounded-lg font-medium hover:bg-background/90 transition-colors"
          >
            Get Your Price
          </Link>
        </div>
      </section>
    </main>
  );
}
```

